{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
      {
        "src": "/api/index.php",
        "dest": "/api/index.php"
      },
      {
        "src": "/styles.css",
        "dest": "/public/styles.css"
      },
      {
        "src": "/image/1.jpg",
        "dest": "/public/image/1.jpg"
      },
      {
         "src": "/(.*)",
         "dest": "/api/index.php"
      }
    ]
}